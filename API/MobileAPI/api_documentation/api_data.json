          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "textReview",
            "description": "<p>The text review itself.</p>"
          },
          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "updatedAt",
            "description": "<p>Timestamp of last modification</p>"
          },
          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "createdAt",
            "description": "<p>Timestamp of creation</p>"
          },
          {
            "group": "Success 200",
            "type": "Image[]",
